int temp;
for(int i=0;i<4;i++) {
	for(int j=1;j<5;j++) {
		if(score[i] > score[j]) {

			temp = score[i];
			score[i]=score[j];
			score[j]=temp;

			temp = indexed[i];
			indexed[i]=indexed[j];
			indexed[j]=temp;
		}
	}
}
for(int i=0;i<4;i++) {
	scoreString += ( playerHandlers[indexed[i]].getPlayerLogin() + ": " + score[indexed[i]] + " ";
}
